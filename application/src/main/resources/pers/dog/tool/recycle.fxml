<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="600.0" prefWidth="760.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pers.dog.api.controller.tool.RecycleController">
    <padding>
        <Insets bottom="16" left="16" right="16" top="16" />
    </padding>
    <HBox alignment="CENTER_LEFT">
        <VBox.margin>
            <Insets bottom="4" />
        </VBox.margin>
        <Label text="%info.common.search" />
        <TextField fx:id="searchTextField" prefWidth="120">
            <HBox.margin>
                <Insets left="4" />
            </HBox.margin>
        </TextField>
    </HBox>
   <TableView>
     <columns>
       <TableColumn prefWidth="160.0" text="%info.project.recycle.project-name" />
       <TableColumn prefWidth="100.0" text="%info.project.recycle.file-type" />
       <TableColumn prefWidth="280.0" text="%info.project.recycle.location" />
       <TableColumn prefWidth="120.0" text="%info.project.recycle.delete-date-time" />
       <TableColumn text="%info.project.recycle.operate" />
     </columns>
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
   </TableView>
</VBox>
